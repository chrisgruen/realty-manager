{
	"name" : "chrisgruen/realty-manager",
	"type" : "typo3-cms-extension",
	"description" : "TYPO3 extension for v10 LTS (version before created by Olicer Klee) that provides a plugin that displays realty objects (properties, real estate), including an image gallery for each object.",
	"homepage" : "https://github.com/chrisgruen/realty-manager",
	"license" : "GPL-2.0-or-later",
	"minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Christian Gruenwald",
            "email": "cg@romonta-schach.de",
            "homepage": "https://demochris.de/",
            "role": "developer"
        }
    ],
	"keywords" : [
		"TYPO3 CMS",
		"realty-manager", 
		"real estate"
	],
	"require" : {
		"bk2k/bootstrap-package" : "^11.0",
		"netresearch/rte-ckeditor-image" : "^10.1"
	},
	"autoload" : {
		"psr-4" : {
			"ChrisGruen\\SimpleNews\\" : "Classes/"
		}
	},
	"config": {
        "optimize-autoloader": true,
        "vendor-dir": ".build/vendor",
        "bin-dir": ".build/bin"
    },
    "extra": {
		"branch-alias": {
			"dev-master": "10.x-dev"
		},
		"typo3/cms": {
			"extension-key": "realty_manager",
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}